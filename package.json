{
  "private": true,
  "name": "gatsby-starter-skeleton",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lint:js": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:style": "stylelint 'src/**/*.{ts,tsx,js,jsx}'",
    "lint:prettier": "prettier 'src/**/*.{json,md}' --list-different",
    "fix": "yarn fix:js && yarn fix:prettier",
    "fix:js": "yarn lint:js --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "type-check": "tsc"
  },
  "dependencies": {
    "@msallent/gatsby-theme-skeleton-core": "1.0.2",
    "@msallent/gatsby-theme-skeleton-seo": "1.0.2",
    "babel-plugin-styled-components": "1.11.1",
    "gatsby": "2.24.11",
    "gatsby-image": "2.4.13",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-sharp": "2.6.22",
    "gatsby-plugin-styled-components": "3.3.10",
    "gatsby-source-filesystem": "2.3.23",
    "gatsby-transformer-sharp": "2.5.11",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "5.1.1"
  },
  "devDependencies": {
    "@types/node": "13.13.15",
    "@types/react": "16.9.44",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.0.0",
    "@types/styled-components": "5.1.2",
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "@typescript-eslint/parser": "3.6.1",
    "eslint": "7.6.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.4",
    "eslint-plugin-react-hooks": "4.0.8",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "stylelint": "13.6.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && lint-staged",
      "post-commit": "git reset"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "stylelint"
    ],
    "src/**/*.{json,md}": "prettier --write"
  }
}
