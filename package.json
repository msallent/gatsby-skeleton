{
  "name": "gatsby-starter-skeleton",
  "license": "MIT",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "fix": "yarn fix:js && yarn fix:scss && yarn fix:prettier",
    "fix:js": "yarn lint:js --fix",
    "fix:scss": "yarn lint:scss --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "lint": "yarn lint:js && yarn lint:scss && yarn lint:prettier",
    "lint:js": "eslint --ignore-path .gitignore . --ext js --ext ts --ext tsx",
    "lint:scss": "stylelint --ignore-path .gitignore 'src/**/*.scss'",
    "lint:prettier": "prettier --ignore-path .gitignore --check .",
    "serve": "gatsby serve",
    "type-check": "tsc"
  },
  "dependencies": {
    "gatsby": "2.24.52",
    "gatsby-image": "2.4.16",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-sass": "2.3.12",
    "gatsby-plugin-sharp": "2.6.30",
    "gatsby-plugin-sitemap": "2.4.12",
    "gatsby-plugin-svgr": "2.0.2",
    "gatsby-source-filesystem": "2.3.27",
    "gatsby-transformer-sharp": "2.5.13",
    "node-sass": "4.14.1",
    "normalize.css": "8.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0"
  },
  "devDependencies": {
    "@svgr/webpack": "5.4.0",
    "@types/node": "13.13.21",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.1.0",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "eslint": "7.9.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.2",
    "gatsby-plugin-ts-config": "1.0.0",
    "husky": "4.3.0",
    "lint-staged": "10.3.0",
    "prettier": "2.1.1",
    "stylelint": "13.7.1",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-scss": "3.18.0",
    "typescript": "4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && lint-staged",
      "post-commit": "git reset"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": "eslint --fix",
    "src/**/*.scss": [
      "prettier --write",
      "stylelint --fix"
    ]
  }
}
